	<Replace Key="OUT_PATH" Value="view" />
	<Replace Key="RAY_COUNT" Value="64" />
	<Replace Key="EXPORT_SVG" Value="yes" />
	<Replace Key="EXPORT_X3D" Value="yes" />
	<Replace Key="EXPORT_DATA" Value="no" />
	<Replace Key="EXPORT_HISTOGRAM" Value="no" />
  </ValueSet>
  <ValueSet Tag="Statistics">
	<Replace Key="OUT_PATH" Value="out" />
   	<Replace Key="RAY_COUNT" Value="500000" />
	<Replace Key="EXPORT_SVG" Value="no" />
	<Replace Key="EXPORT_X3D" Value="no" />
	<Replace Key="EXPORT_DATA" Value="yes" />
	<Replace Key="EXPORT_HISTOGRAM" Value="yes" />
