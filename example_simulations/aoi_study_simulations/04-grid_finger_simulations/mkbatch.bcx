<BatchConfig XMLSource="grid_fingers_setup.scx" ID="grid_fingers" OutputDir="grid_finger_scatter_histogram" Key="@TRAPEZOID_OFFSET@%@LIGHT_ANGLE@%@ROUGHNESS_DEVIATION_ANGLE@%@RAY_COUNT@">

